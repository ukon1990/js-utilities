{
  "name": "@ukon1990/js-utilities",
  "version": "1.9.0",
  "description": "A light weight package for object and array manipulation. As well as some utilities for matching text.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "npm test -- --watch --coverage=false",
    "deploy": "npm run lint && npm run test && tsc && npm publish --access public",
    "lint": "tslint --project tsconfig.json"
  },
  "keywords": [
    "String",
    "Text",
    "Object",
    "Array",
    "Compare",
    "Clone",
    "Difference",
    "Equality",
    "isEmpty",
    "CSV"
  ],
  "author": "Jonas Munthe Fl√∏nes",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukon1990/js-utilities"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
